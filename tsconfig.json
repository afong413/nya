{
  "compilerOptions": {
    "lib": ["ES2020", "DOM"],
    "target": "ES2020",
    "strict": true,
    "noUncheckedIndexedAccess": true,
    "noEmit": true,
    "skipDefaultLibCheck": true,
    "noFallthroughCasesInSwitch": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "moduleResolution": "bundler"
  },
  "include": [
    "src/eval",
    "src/field",
    "src/sheet",
    "src/index.ts",
    "src/jsx.ts",
    "src/stack.ts"
  ]
}
